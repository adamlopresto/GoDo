<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <item
        android:left="4dp"
        android:right="4dp"
        android:top="4dp"
        android:bottom="4dp"
        >
        <shape
            android:shape="rectangle"
            >
            <corners
                android:radius="5dp"
                />
            <solid
                android:color="@android:color/white"
                />
            <stroke
                android:color="@android:color/black"
                android:width="1dp"
                />
            <size
                android:height="32sp"
                android:width="32sp"
                />
        </shape>
    </item>
    <item
        android:top="7sp"
        android:bottom="0sp"
        android:left="4sp"
        android:right="16sp"
        >
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape
                android:shape="line"
                >
            <stroke
                android:color="#007000"
                android:width="3sp"
                />
                <size
                    android:width="1dp"
                    android:height="10dp"
                    />
            </shape>
            </rotate>
    </item>
    <item
        android:top="-4sp"
        android:bottom="-2sp"
        android:left="13sp"
        android:right="-4sp"
        >
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="-45">
            <shape
                android:shape="line"
                >
                <corners android:radius="1dp"/>
                <stroke
                    android:color="#007000"
                    android:width="3sp"
                    />
                <size
                    android:width="1dp"
                    android:height="10dp"
                    />
            </shape>
        </rotate>
    </item>
</layer-list>